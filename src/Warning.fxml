<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>


<Pane>
   <children>
      <Label layoutX="13.0" layoutY="66.0" text="The file is not saved. Do you want to save it?">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="143.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Yes" onAction="#yes"/>
      <Button layoutX="115.0" layoutY="143.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="No" onAction="#no"/>
      <Button layoutX="216.0" layoutY="143.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" text="Cancel" onAction="#cancel"/>
   </children>
</Pane>
